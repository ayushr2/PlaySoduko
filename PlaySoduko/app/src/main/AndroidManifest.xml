<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ayush.playsoduko.playsoduko">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/best"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ayush.playsoduko.playsoduko.Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ayush.playsoduko.playsoduko.Play"></activity>
        <activity android:name="com.ayush.playsoduko.playsoduko.Solve"
            android:noHistory="true"></activity>
        <activity android:name="com.ayush.playsoduko.playsoduko.Pop"
            android:theme="@style/AppTheme.keyPadCustom">
        </activity>
        <activity android:name="com.ayush.playsoduko.playsoduko.PlaySingle"
            android:noHistory="true"></activity>
        <activity android:name="com.ayush.playsoduko.playsoduko.PopSinglePlay"
            android:theme="@style/AppTheme.keyPadCustom">
        </activity>
        <activity android:name="com.ayush.playsoduko.playsoduko.PlayOnline"
            android:noHistory="true"></activity>
        <activity android:name="com.ayush.playsoduko.playsoduko.PopOnlinePlay"
            android:theme="@style/AppTheme.keyPadCustom">
        </activity>
        <activity android:name="com.ayush.playsoduko.playsoduko.Win">
        </activity>
    </application>

</manifest>