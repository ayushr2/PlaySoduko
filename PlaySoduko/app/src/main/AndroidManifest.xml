<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.playsoduko.playsoduko">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Play"></activity>
        <activity android:name=".Solve"
            android:noHistory="true"></activity>
        <activity android:name=".Pop"
            android:theme="@style/AppTheme.keyPadCustom">
        </activity>
        <activity android:name=".PlaySingle"
            android:noHistory="true"></activity>
        <activity android:name=".PopSinglePlay"
            android:theme="@style/AppTheme.keyPadCustom">
        </activity>
    </application>

</manifest>